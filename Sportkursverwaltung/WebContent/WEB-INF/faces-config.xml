<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<application>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>de</supported-locale>
		</locale-config>
	</application>
	<managed-bean>
		<managed-bean-name>user</managed-bean-name>
		<managed-bean-class>model.User</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>meinProfilController</managed-bean-name>
		<managed-bean-class>controller.MeinProfilController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>user</property-name>
			<property-class>model.User</property-class>
			<value>#{user}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>kurseController</managed-bean-name>
		<managed-bean-class>controller.KurseController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>user</property-name>
			<property-class>model.User</property-class>
			<value>#{user}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginController</managed-bean-name>
		<managed-bean-class>controller.LoginController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>user</property-name>
			<property-class>model.User</property-class>
			<value>#{user}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>meineTermineController</managed-bean-name>
		<managed-bean-class>controller.MeineTermineController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>user</property-name>
			<property-class>model.User</property-class>
			<value>#{user}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>trainerbereichController</managed-bean-name>
		<managed-bean-class>controller.TrainerbereichController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>kurse</from-outcome>
			<to-view-id>/kurse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>trainer</from-outcome>
			<to-view-id>/trainerbereich.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>kurse.xhtml</display-name>
		<from-view-id>/kurse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>termine</from-outcome>
			<to-view-id>/termine.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>termine.xhtml</display-name>
		<from-view-id>/termine.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>profil</from-outcome>
			<to-view-id>/profil.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>profil.xhtml</display-name>
		<from-view-id>/profil.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>trainer</from-outcome>
			<to-view-id>/trainerbereich.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>kurse.xhtml</display-name>
		<from-view-id>/kurse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>trainer</from-outcome>
			<to-view-id>/trainerbereich.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>kurse.xhtml</display-name>
		<from-view-id>/kurse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>profil</from-outcome>
			<to-view-id>/profil.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>termine.xhtml</display-name>
		<from-view-id>/termine.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>trainer</from-outcome>
			<to-view-id>/trainerbereich.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>trainerbereich.xhtml</display-name>
		<from-view-id>/trainerbereich.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>kurse</from-outcome>
			<to-view-id>/kurse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>trainerbereich.xhtml</display-name>
		<from-view-id>/trainerbereich.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>profil</from-outcome>
			<to-view-id>/profil.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>profil.xhtml</display-name>
		<from-view-id>/profil.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>termine</from-outcome>
			<to-view-id>/termine.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>termine.xhtml</display-name>
		<from-view-id>/termine.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>kurse</from-outcome>
			<to-view-id>/kurse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>trainerbereich.xhtml</display-name>
		<from-view-id>/trainerbereich.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>termine</from-outcome>
			<to-view-id>/termine.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>profil.xhtml</display-name>
		<from-view-id>/profil.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>kurse</from-outcome>
			<to-view-id>/kurse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>kurse.xhtml</display-name>
		<from-view-id>/kurse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>trainerbereich.xhtml</display-name>
		<from-view-id>/trainerbereich.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>profil.xhtml</display-name>
		<from-view-id>/profil.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>termine.xhtml</display-name>
		<from-view-id>/termine.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<validator>
		<display-name>validator.LoginValidator</display-name>
		<validator-id>validator.LoginValidator</validator-id>
		<validator-class>validator.LoginValidator</validator-class>
	</validator>

</faces-config>
